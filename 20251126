    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "ì „ì²´ ì´ë¯¸ì§€ ìˆ˜: 7481\n",
      "train: 4488  / val: 2993\n",
      "ë°ì´í„° ë¶„ë¦¬ ì™„ë£Œ!\n",
      "images_yolo/train: 4488\n",
      "images_yolo/val  : 2993\n",
      "labels_yolo/train: 4488\n",
      "labels_yolo/val  : 2993\n"
     ]
    }
   ],
   "source": [
    "import os\n",
    "import random\n",
    "import shutil\n",
    "\n",
    "BASE_DIR = \"kitti\"\n",
    "IMG_ROOT = os.path.join(BASE_DIR, \"images_yolo\")\n",
    "LBL_ROOT = os.path.join(BASE_DIR, \"labels_yolo\")\n",
    "\n",
    "IMG_TRAIN = os.path.join(IMG_ROOT, \"train\")\n",
    "IMG_VAL   = os.path.join(IMG_ROOT, \"val\")\n",
    "LBL_TRAIN = os.path.join(LBL_ROOT, \"train\")\n",
    "LBL_VAL   = os.path.join(LBL_ROOT, \"val\")\n",
    "\n",
    "os.makedirs(IMG_TRAIN, exist_ok=True)\n",
    "os.makedirs(IMG_VAL, exist_ok=True)\n",
    "os.makedirs(LBL_TRAIN, exist_ok=True)\n",
    "os.makedirs(LBL_VAL, exist_ok=True)\n",
    "\n",
    "# ì´ë¯¸ì§€ íŒŒì¼ ëª©ë¡ ê°€ì ¸ì˜¤ê¸° (train/val í´ë” ì œì™¸)\n",
    "all_imgs = [\n",
    "    f for f in os.listdir(IMG_ROOT)\n",
    "    if f.endswith((\".png\", \".jpg\")) and\n",
    "       os.path.isfile(os.path.join(IMG_ROOT, f))\n",
    "]\n",
    "\n",
    "print(\"ì „ì²´ ì´ë¯¸ì§€ ìˆ˜:\", len(all_imgs))\n",
    "\n",
    "# 6:4 split\n",
    "random.seed(42)\n",
    "random.shuffle(all_imgs)\n",
    "\n",
    "n_total = len(all_imgs)\n",
    "n_train = int(n_total * 0.6)\n",
    "train_files = all_imgs[:n_train]\n",
    "val_files   = all_imgs[n_train:]\n",
    "\n",
    "print(\"train:\", len(train_files), \" / val:\", len(val_files))\n",
    "\n",
    "def move_pair(img_name, dst_img_dir, dst_lbl_dir):\n",
    "    src_img_path = os.path.join(IMG_ROOT, img_name)\n",
    "    src_lbl_name = os.path.splitext(img_name)[0] + \".txt\"\n",
    "    src_lbl_path = os.path.join(LBL_ROOT, src_lbl_name)\n",
    "\n",
    "    dst_img_path = os.path.join(dst_img_dir, img_name)\n",
    "    dst_lbl_path = os.path.join(dst_lbl_dir, src_lbl_name)\n",
    "\n",
    "    # ì´ë¯¸ì§€ ì´ë™\n",
    "    shutil.move(src_img_path, dst_img_path)\n",
    "\n",
    "    # ë¼ë²¨ì´ ì¡´ì¬í•˜ëŠ” ê²½ìš°ë§Œ ì´ë™ (ì—†ìœ¼ë©´ ê·¸ëƒ¥ ë¬´ì‹œ)\n",
    "    if os.path.exists(src_lbl_path):\n",
    "        shutil.move(src_lbl_path, dst_lbl_path)\n",
    "\n",
    "# ì‹¤ì œ ì´ë™\n",
    "for name in train_files:\n",
    "    move_pair(name, IMG_TRAIN, LBL_TRAIN)\n",
    "\n",
    "for name in val_files:\n",
    "    move_pair(name, IMG_VAL, LBL_VAL)\n",
    "\n",
    "print(\"ë°ì´í„° ë¶„ë¦¬ ì™„ë£Œ!\")\n",
    "print(\"images_yolo/train:\", len(os.listdir(IMG_TRAIN)))\n",
    "print(\"images_yolo/val  :\", len(os.listdir(IMG_VAL)))\n",
    "print(\"labels_yolo/train:\", len(os.listdir(LBL_TRAIN)))\n",
    "print(\"labels_yolo/val  :\", len(os.listdir(LBL_VAL)))\n"
   ]
  },
