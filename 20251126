    "import time\n",
    "\n",
    "# ê²€ì¦ìš© ì´ë¯¸ì§€ í•˜ë‚˜ ì„ íƒ\n",
    "img = random.choice(val_imgs)\n",
    "print(\"FPS ì¸¡ì •ìš© ì´ë¯¸ì§€:\", img)\n",
    "\n",
    "# ì›Œë°ì—…\n",
    "_ = best_model.predict(source=img, device=\"mps\", verbose=False)\n",
    "\n",
    "N = 50\n",
    "start = time.time()\n",
    "for _ in range(N):\n",
    "    _ = best_model.predict(source=img, device=\"mps\", verbose=False)\n",
    "end = time.time()\n",
    "\n",
    "fps = N / (end - start)\n",
    "print(f\"ì¶”ë¡  FPS: {fps:.2f}\")\n"
