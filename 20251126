   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "import os\n",
    "import cv2\n",
    "from tqdm.auto import tqdm  # ë§¨ ìœ„ importë“¤ ì˜†ì— ì¶”ê°€\n",
    "\n",
    "# ----- ê²½ë¡œ ì„¤ì • -----\n",
    "IMG_DIR = \"data_object_image_2/training/image_2\"\n",
    "LBL_DIR = \"training/label_2\"\n",
    "\n",
    "OUT_IMG = \"kitti/images_yolo\"      # YOLO ì´ë¯¸ì§€ ì €ì¥\n",
    "OUT_LBL = \"kitti/labels_yolo\"      # YOLO ë¼ë²¨ ì €ì¥\n",
    "\n",
    "os.makedirs(OUT_IMG, exist_ok=True)\n",
    "os.makedirs(OUT_LBL, exist_ok=True)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "application/vnd.jupyter.widget-view+json": {
       "model_id": "53256dd5dc5a4674a3d0bdd3add06002",
       "version_major": 2,
       "version_minor": 0
      },
      "text/plain": [
       "Converting KITTI â†’ YOLO:   0%|          | 0/7481 [00:00<?, ?it/s]"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "\n",
    "# ----- í´ë˜ìŠ¤ ë§¤í•‘  -----\n",
    "def map_class(name: str):\n",
    "    name = name.lower()\n",
    "\n",
    "    # ì°¨ëŸ‰ ê³„ì—´\n",
    "    if name in [\"car\", \"van\", \"truck\"]:\n",
    "        return 0\n",
    "\n",
    "    # ë³´í–‰ì\n",
    "    if name in [\"pedestrian\"]:\n",
    "        return 1\n",
    "\n",
    "    # ì´ë¥œì°¨\n",
    "    if name in [\"cyclist\"]:\n",
    "        return 2\n",
    "\n",
    "    # ì œê±°í•  í´ë˜ìŠ¤ë“¤\n",
    "    if name in [\"person_sitting\", \"dontcare\", \"misc\", \"tram\"]:\n",
    "        return None\n",
    "\n",
    "    # ê·¸ ì™¸ë„ ì „ë¶€ ë¬´ì‹œ\n",
    "    return None\n",
    "\n",
    "\n",
    "# ----- ë³€í™˜ í•¨ìˆ˜ -----\n",
    "def convert_kitti_to_yolo(img_name):\n",
    "    img_path = os.path.join(IMG_DIR, img_name)\n",
    "    label_name = img_name.replace(\".png\", \".txt\").replace(\".jpg\", \".txt\")\n",
    "    label_path = os.path.join(LBL_DIR, label_name)\n",
    "\n",
    "    img = cv2.imread(img_path)\n",
    "    if img is None:\n",
    "        return\n",
    "\n",
    "    h, w = img.shape[:2]\n",
    "    yolo_lines = []\n",
    "\n",
    "    if os.path.exists(label_path):\n",
    "        with open(label_path, \"r\") as f:\n",
    "            for line in f:\n",
    "                parts = line.strip().split()\n",
    "                cls_name = parts[0]\n",
    "\n",
    "                cls_id = map_class(cls_name)\n",
    "                if cls_id is None:\n",
    "                    continue\n",
    "\n",
    "                xmin = float(parts[4])\n",
    "                ymin = float(parts[5])\n",
    "                xmax = float(parts[6])\n",
    "                ymax = float(parts[7])\n",
    "\n",
    "                x_center = ((xmin + xmax) / 2) / w\n",
    "                y_center = ((ymin + ymax) / 2) / h\n",
    "                bw = (xmax - xmin) / w\n",
    "                bh = (ymax - ymin) / h\n",
    "\n",
    "                if bw <= 0 or bh <= 0:\n",
    "                    continue\n",
    "\n",
    "                yolo_lines.append(f\"{cls_id} {x_center} {y_center} {bw} {bh}\\n\")\n",
    "\n",
    "    # ì €ì¥\n",
    "    out_img_path = os.path.join(OUT_IMG, img_name)\n",
    "    out_lbl_path = os.path.join(OUT_LBL, label_name)\n",
    "\n",
    "    cv2.imwrite(out_img_path, img)\n",
    "    with open(out_lbl_path, \"w\") as f:\n",
    "        f.writelines(yolo_lines)\n",
    "\n",
    "\n",
    "# íŒŒì¼ ëª©ë¡\n",
    "files = sorted([f for f in os.listdir(IMG_DIR)\n",
    "                if f.endswith(\".png\") or f.endswith(\".jpg\")])\n",
    "\n",
    "# ğŸ”¥ tqdmìœ¼ë¡œ ì§„í–‰ë¥  í‘œì‹œ\n",
    "for name in tqdm(files, desc=\"Converting KITTI â†’ YOLO\"):\n",
    "    convert_kitti_to_yolo(name)\n",
    "\n",
    "print(\"KITTI â†’ YOLO ë³€í™˜ ì™„ë£Œ!\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "**ë¼ë²¨ ë³€í™˜ì´ ì˜ ë˜ì—ˆëŠ”ì§€ ì‹¤ì œ ì´ë¯¸ì§€ì— ë°•ìŠ¤ ê·¸ë ¤ì„œ í™•ì¸í•˜ëŠ” ì‹œê°í™”**"
